package com.company;

public class Employee {
    private int id;
    private  String name;

    public Employee(int id, String name)
    {
        super();
        this.id=id;
        this.name=name;
    }
    public int getId()
    {
        return id;
    }
    public String getName()
    {
        return name;
    }

    @Override
    public int hashCode()
    {
        return id;
    }

    @Override
    public boolean equals(Object obj)
    {
        Employee e2=(Employee)obj;
        if(this.id==e2.id && this.name.equals(e2.name))
        {
            return true;
        }
        return false;

    }
    @Override
    public String toString()
    {
      return id+" "+name;
    }
}

